<launch>

    <arg name="model" default="$(find rpr_arm)/urdf/rpr_arm.urdf"/>

    <include file="$(find rpr_arm)/launch/gazebo.launch">
        
    </include>

    <include file="$(find rpr_arm)/launch/controller.launch">
        <arg name="is_sim" value="True"/>
    </include>

    <include file="$(find rpr_moveit)/launch/move_group.launch">
    </include>

    <include file="$(find rpr_moveit)/launch/moveit_rviz.launch">
        <arg name="rviz_config" value="$(find rpr_moveit)/launch/moveit.rviz"/>
    </include>

    <include file="$(find rpr_arm)/launch/alexa_control.launch"/>

</launch>